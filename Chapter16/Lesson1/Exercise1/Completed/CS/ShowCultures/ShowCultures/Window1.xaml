<Window x:Class="ShowCultures.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Culture Browser" Height="335" Width="300" Loaded="Window_Loaded">
    <Grid>
        <ComboBox Height="23" Margin="13,16,19,0" Name="cultureComboBox" VerticalAlignment="Top" SelectionChanged="cultureComboBox_SelectionChanged" />
        <Label Margin="13,47,0,0" Name="label1" Height="28" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120">Code:</Label>
        <Label Height="28" Margin="13,70,0,0" Name="label2" VerticalAlignment="Top" HorizontalAlignment="Left" Width="120">Native:</Label>
        <Label Height="28" Margin="13,98,0,0" Name="label5" VerticalAlignment="Top" HorizontalAlignment="Left" Width="120">Date:</Label>
        <Label Height="28" Margin="104,47,19,0" Name="codeLabel" VerticalAlignment="Top"></Label>
        <Label Height="28" Margin="104,70,19,0" Name="nativeLabel" VerticalAlignment="Top"></Label>
        <Label Height="28" Margin="104,98,19,0" Name="dateLabel" VerticalAlignment="Top"></Label>
        <Label Margin="104,122,19,112" Name="currencyLabel"></Label>
        <Label HorizontalAlignment="Left" Margin="13,122,0,131" Name="label3" Width="120">Currency:</Label>
    </Grid>
</Window>
